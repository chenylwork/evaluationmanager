@charset "UTF-8";

*{
	margin: 0 auto;
	padding: 0;
}
body{
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
}
ul li{
	width: 800px;
	height: 70px;
	list-style: none;
	padding-left: 25px;
}
	
.main{
	width: 100%;
	height: 550px;
	padding-top: 50px;
	float: left;
	
}
.left{
	width:350px;
	height: 350px;
	padding-left: 50px;
	float: left;
}
.right{
	width:500px;
	height: 350px;
	padding-top: 40px;
	float: left;
}
.required{
	width: 300px;
	border-radius: 3px;
	border:1px solid #ccc;
	height:35px;
	font-size: 12px;
	padding-left: 5px;
}

.onError{
	color: #666;
    background: url(../../../images/reg3.gif) no-repeat 0 center;
	padding-left:25px;
}
.onSuccess{
	color: #fff;
    background: url(../../../images/reg4.gif) no-repeat 0 center;
	padding-left:25px;
}
.btn1{
	width:80px;
	height:25px;
	background-color:#000;
	color:#FFFFFF;
	border-radius:5px;
	font-size:14px;
	font-weight:bold;
}
