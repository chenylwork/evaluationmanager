package com.evaluationmanager.entiy;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;
import javax.persistence.UniqueConstraint;

import com.evaluationmanager.annotation.Foreignkey;
import com.evaluationmanager.annotation.No;

/**
 * 
 * @Author 韩笑
 * @Datetime 2018年8月9日-上午10:55:59
 * @Description 描述信息：权限信息实体类
 *
 */
@Entity
@Table(name="permissioninfo",uniqueConstraints = {@UniqueConstraint(columnNames="permisInfoNo"),})
public class PermissionInfo {
	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int id;
	@No
	private String permisInfoNo;
	private String name;
	private String uri;
	@Foreignkey(foreignClass=PermissionType.class,column="permisTypeNo")
	private String type;
	
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}


	public String getPermisInfoNo() {
		return permisInfoNo;
	}

	public void setPermisInfoNo(String permisInfoNo) {
		this.permisInfoNo = permisInfoNo;
	}

	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public String getUri() {
		return uri;
	}


	public void setUri(String uri) {
		this.uri = uri;
	}


	public String getType() {
		return type;
	}


	public void setType(String type) {
		this.type = type;
	}


	@Override
	public String toString() {
		return "PermissionInfo [id=" + id + ", permisInfoNo=" + permisInfoNo + ", name=" + name + ", uri=" + uri
				+ ", type=" + type + "]";
	}
	
	
}
