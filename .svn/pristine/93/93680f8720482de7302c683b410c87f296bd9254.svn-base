@CHARSET "UTF-8";
*{
	margin: 0 auto;
	padding: 0;
}
body{
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
}
ul li{
	width: 700px;
	height: 50px;
	list-style: none;
	font-size:14px;
}
.main{
	width: 100%;
	height: 500px;
	float: left;
	
}
.left{
	width:300px;
	height: 350px;
	padding-left: 50px;
	padding-top: 200px;
	float: left;
}
.right{
	width:480px;
	height: 350px;
	float: left;
}
.required{
	width: 250px;
	height:32px;
	border:1px solid #ccc;
	border-radius: 3px;
	font-size: 12px;
	padding-left: 5px;
}

.onError{
	color: #666;
    background: url(../../../images/reg3.gif) no-repeat 0 center;
	padding-left:25px;
}
.onSuccess{ 
	color: #fff;
    background: url(../../../images/reg4.gif) no-repeat 0 center;
	padding-left:25px;
}
span{
	font-size:14px;
}
.btn1{
	width:80px;
	height:25px;
	background-color:#000;
	color:#FFFFFF;
	border-radius:5px;
	font-size:14px;
	font-weight:bold;
}
