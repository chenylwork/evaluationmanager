<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<!--创建人：任少华 -->
<!--创建时间：2018年7月30日-上午:3:25:10 -->
<!--配置信息：major问题模块的配置信息 -->
<struts>
<constant name="struts.devMode" value="true" />
    <constant name="struts.i18n.encoding" value="UTF-8" /> 
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <package name="log" namespace="/admin/log" extends="struts-default,json-default">
        <!-- 学生模块-->
         <action name="*" class="com.evaluationmanager.action.LogAction" method="{1}">
         	<!-- 添加登录日志 -->
            <result name="addLog"></result>
            <!-- 删除登录日志 -->
            <result name="deleteLog">/admin/logfile/logon_log_list.jsp</result>
            <result name="updateLog"></result>
            <!-- 查询全部登录日志 -->
            <result name="allLog">/admin/logfile/logon_log_list.jsp</result>
            <result name=""></result>
         </action>
         
         
    </package>
</struts>